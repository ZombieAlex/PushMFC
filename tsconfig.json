{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": false,
        "outDir": "./lib",
        "sourceMap": true,
        "noLib": false,
        "moduleResolution": "node",
        "stripInternal": true,
        "removeComments": true
    },
    "exclude": [
        "lib",
        "node_modules"
    ]
}